<form method="POST" class="st_input">
  <fieldset>
    <legend>학생정보</legend>
    <div>
      <label for="st_num">학번</label
      ><input name="st_num" id="st_num" placeholder="학번" />
    </div>
    <div>
      <label for="st_name">이름</label
      ><input name="st_name" id="st_name" placeholder="이름" />
    </div>
    <div>
      <label for="st_dept">학과</label
      ><input name="st_dept" id="st_dept" placeholder="학과" />
    </div>
    <div>
      <label for="st_grade">학년</label
      ><input type="number" name="st_grade" id="st_grade" placeholder="학년" />
    </div>
    <div>
      <label for="st_tel">전화번호</label
      ><input type="phone" name="st_tel" id="st_tel" placeholder="전화번호" />
    </div>
    <div>
      <label for="st_addr">주소</label
      ><input name="st_addr" id="st_addr" placeholder="주소" />
    </div>
    <div>
      <label></label>
      <button type="button" class="btn submit">저장</button>
    </div>
  </fieldset>
</form>
